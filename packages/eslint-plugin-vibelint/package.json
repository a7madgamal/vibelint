{
  "name": "@vibelint/eslint-plugin-vibelint",
  "version": "0.1.13",
  "description": "ESLint plugin to suppress approved warnings based on cache file",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "warnings",
    "suppress",
    "approval"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  }
}
